{
  "name": "food-delivery-client",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node ./dist/server/server.js",
    "start:dev": "ng serve",
    "build": "ng build && tsc -p tsconfig.server.json",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.1.0",
    "@angular/common": "^14.1.0",
    "@angular/compiler": "^14.1.0",
    "@angular/core": "^14.1.0",
    "@angular/forms": "^14.1.0",
    "@angular/localize": "^14.1.0",
    "@angular/platform-browser": "^14.1.0",
    "@angular/platform-browser-dynamic": "^14.1.0",
    "@angular/router": "^14.1.0",
    "@auth0/angular-jwt": "^5.0.2",
    "@ng-bootstrap/ng-bootstrap": "^13.0.0",
    "@ng-select/ng-select": "^9.0.2",
    "@types/bootstrap": "^5.2.4",
    "@types/openlayers": "^4.6.18",
    "bootstrap": "^5.2.0",
    "bootstrap-icons": "^1.9.1",
    "express": "^4.18.1",
    "http-proxy-middleware": "^2.0.6",
    "ngx-drag-scroll": "^14.0.1",
    "ol": "^7.1.0",
    "rxjs": "~7.5.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.1.3",
    "@angular/cli": "~14.1.3",
    "@angular/compiler-cli": "^14.1.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.8",
    "jest": "^28.1.3",
    "jest-preset-angular": "^12.2.0",
    "tslib": "^2.4.0",
    "typescript": "~4.7.2"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setup-jest.ts"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(ol|@angular|@ng-bootstrap|ngx-drag-scroll|@ng-select)/)"
    ]
  }
}